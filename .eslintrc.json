{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
    "next",
    "prettier",
    "plugin:prettier/recommended"
  ],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["react","prettier"],
  "rules": {
    "comma-dangle": "off",
    "no-unused-vars": 1
    // "prettier/prettier": [
    //   "error",{},
    //   {"usePrettierrc":true
    //   }
    // ]
  }
}
